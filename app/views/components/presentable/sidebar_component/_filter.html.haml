%ul.list-group
  - fields.each do |field|
    - object.send(field).each do |element|
      %li.list-group-item
        - name = nested.present? && nested[field.to_sym].present? ? element.send(nested[field.to_sym]) : element
        %span
          = link_to name, services_filter_link(element, name, filter_query[field.to_sym])



