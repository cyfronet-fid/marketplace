%li.label
  = _("Services")
- results.each do |elem, highlights|
  - if highlights[:name]
    = render "services/autocomplete/service_item", service: elem, highlights: highlights
%li.label
  = _("Datasources")
- results.each do |elem, highlights|
  - if highlights[:datasource_name]
    = render "services/autocomplete/datasource_item", datasource: elem, highlights: highlights
%li.label
  = _("Offers")
- results.each do |elem, highlights|
  - if highlights[:offer_name]
    = render "services/autocomplete/offer_item", offer: elem,
      highlights: highlights,
      service_titles: service_titles
%li.label
  = _("Providers")
- results.each do |elem, highlights|
  - if highlights[:provider_name]
    = render "services/autocomplete/provider_item", provider: elem,
     highlights: highlights
