- unless @project_item.property_values.blank?
  %h5.mb-3.font-weight-bold.configuration-title.text-uppercase Technical parameters

  - project_item.property_values.each do |attr|
    = f.simple_fields_for :property_values, attr do |field|
      - begin
        - puts "rendering #{attr.inspect}"
        = render "services/configurations/attributes/#{attr.type}", attr: attr, field: field
      - rescue => e
        = e.message

